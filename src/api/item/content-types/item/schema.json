{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "status_item": {
      "type": "enumeration",
      "enum": [
        "found",
        "missing"
      ]
    },
    "ownerName": {
      "type": "string"
    },
    "ownerPhone": {
      "type": "string"
    },
    "ownerEmail": {
      "type": "email"
    },
    "location": {
      "type": "text"
    },
    "date": {
      "type": "date"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "items"
    }
  }
}
